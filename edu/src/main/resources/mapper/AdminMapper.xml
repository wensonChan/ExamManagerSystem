<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neu.edu.mapper.AdminMapper">

    <select id="login" resultType="adminVO" parameterType="map">
        select t_admin from t_admin where t_admin = #{t_admin} and password = #{password}
    </select>

</mapper>